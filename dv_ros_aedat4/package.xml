<?xml version="1.0"?>
<package format="2">
  <name>dv_ros_aedat4</name>
  <version>1.3.0</version>
  <description>Converter nodes for converting aedat4 format recordings into rosbag files.</description>

  <maintainer email="support@inivation.com">iniVation</maintainer>

  <license>Apache2.0</license>

  <!-- Url tags are optional, but multiple are allowed, one per tag -->
  <!-- Optional attribute type can be: website, bugtracker, or repository -->
  <!-- Example: -->
  <!-- <url type="website">http://wiki.ros.org/dv_ros_cpp</url> -->

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rospy</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>rosbag</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>dv_ros_msgs</build_depend>
  <build_depend>cv_brigde</build_depend>


  <build_export_depend>rospy</build_export_depend>
  <build_export_depend>std_msgs</build_export_depend>
  <build_export_depend>sensor_msgs</build_export_depend>
  <build_export_depend>dv_ros_msgs</build_export_depend>
  <build_export_depend>cv_brigde</build_export_depend>

  <exec_depend>rospy</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>rosbag</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>dv_ros_msgs</exec_depend>
  <exec_depend>cv_brigde</exec_depend>

</package>
